{
    "name": "makville/cakephp-mailer-plugin",
    "description": "MakvilleMailer plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "mailgun/mailgun-php": "^2.6",
        "php-http/guzzle6-adapter": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MakvilleMailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MakvilleMailer\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
